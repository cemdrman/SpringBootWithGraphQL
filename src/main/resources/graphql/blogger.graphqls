type Blogger{

     id: ID!
	 name: String!
	 surname: String!
	 email: String!
	 password: String!

}

extend type Query {
    getAllBloggers: [Blogger]!
    getBlogger(id: ID!): Blogger!
}


type Mutation {

    saveBlogger(id: ID!, name: String!, surname: String!,  email: String, password: String) : Blogger!
 
}